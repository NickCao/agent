{
  "name": "uhk-agent",
  "private": true,
  "author": "Ultimate Gadget Laboratories",
  "main": "electron/dist/electron-main.js",
  "version": "2.1.1",
  "firmwareVersion": "9.2.1",
  "deviceProtocolVersion": "4.8.0",
  "userConfigVersion": "5.1.0",
  "hardwareConfigVersion": "1.0.0",
  "description": "Agent is the configuration application of the Ultimate Hacking Keyboard.",
  "repository": {
    "type": "git",
    "url": "git@github.com:UltimateHackingKeyboard/agent.git"
  },
  "license": "See in LICENSE",
  "engines": {
    "node": ">=16.17.1 <17.0.0",
    "npm": ">=8.15.0 <9.0.0"
  },
  "workspaces": [
    "packages/*"
  ],
  "devDependencies": {
    "@electron/notarize": "1.2.3",
    "@types/command-line-args": "5.2.0",
    "@types/command-line-usage": "5.0.2",
    "@types/decompress": "4.2.4",
    "@types/electron-devtools-installer": "2.0.2",
    "@types/electron-settings": "4.0.0",
    "@types/file-saver": "2.0.5",
    "@types/jasmine": "4.3.0",
    "@types/lodash": "4.14.168",
    "@types/node": "16.18.2",
    "@types/node-hid": "1.3.1",
    "@types/request": "2.0.8",
    "@types/tmp": "0.2.0",
    "@types/yargs": "17.0.8",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "check-node-version": "4.2.1",
    "copy-webpack-plugin": "10.2.4",
    "copyfiles": "2.4.1",
    "core-js": "3.26.0",
    "cross-env": "7.0.3",
    "decompress": "4.2.1",
    "decompress-targz": "^4.1.1",
    "desm": "1.3.0",
    "devtron": "1.4.0",
    "electron": "22.0.2",
    "electron-builder": "23.6.0",
    "electron-debug": "1.5.0",
    "electron-devtools-installer": "2.2.3",
    "electron-log": "4.4.8",
    "electron-settings": "4.0.2",
    "electron-updater": "5.3.0",
    "eslint": "8.9.0",
    "fs-extra": "10.1.0",
    "jasmine": "4.4.0",
    "lodash": "4.17.21",
    "node-hid": "2.1.2",
    "npm-run-all": "4.1.5",
    "nrf-intel-hex": "1.3.0",
    "nx": ">=14.8.6 < 16",
    "postcss-scss": "4.0.2",
    "request": "2.88.0",
    "rimraf": "3.0.2",
    "source-map-support": "0.5.21",
    "stylelint": "14.0.1",
    "svg-sprite": "2.0.2",
    "ts-loader": "9.2.6",
    "ts-node": "10.9.1",
    "typescript": "4.6.3",
    "webpack": "5.74.0",
    "webpack-cli": "4.10.0"
  },
  "scripts": {
    "test": "npm run test --workspaces",
    "lint": "npm run lint --workspaces",
    "e2e": "npm run e2e --workspace=uhk-web",
    "prebuild": "check-node-version --package",
    "build": "npm run build --workspaces --if-present",
    "build:web": "npm run build:web --workspace=uhk-web",
    "server:web": "npm run start --workspace=uhk-web",
    "server:electron": "npm run server:renderer --workspace=uhk-web",
    "electron": "npm run start --workspace=uhk-agent",
    "electron:spe": "npm run electron:spe --workspace=uhk-agent",
    "pack": "node ./scripts/release.js",
    "sprites": "node ./scripts/generate-svg-sprites",
    "release": "node ./scripts/release.js",
    "clean": "npm run clean --workspaces && rimraf ./node_modules ./dist ./tmp",
    "convert-user-config-to-bin": "cross-env TS_NODE_PROJECT=./packages/usb/tsconfig.json node --loader ts-node/esm ./packages/usb/user-config-json-to-bin.ts"
  }
}
